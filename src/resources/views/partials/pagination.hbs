     <nav aria-label="...">
      <ul class="pagination d-flex justify-content-center ">

        {{#if (equalOr currentPage '1' '')}} <li class="page-item disabled"><span class="page-link">Previous</span>
        </li>
        {{else}}
        <li class="page-item">
          <a class="page-link" href="?page={{previousPage}}">Previous</a>
        </li>
        {{/if}}

        {{#times totalPages}}
        <li class="page-item {{#if (equal @index previousPage)}} active {{else}} {{/if}}">
          <a class="page-link " href="?page={{sum @index 1}}">{{sum @index 1}}</a>
          {{!-- <a class="page-link " href=".">{{sum @index 1}}</a> --}}
          {{!-- <a class="page-link " href="?page={{sum @index 1}}">{{sum @index 1}}</a> --}}

        </li>
        {{/times}}
        {{#if (equal currentPage lastPage)}}<li class="page-item disabled"><span class="page-link">Next Page</span>
        </li>
        {{else}}

        <li class="page-item">
          <a class="page-link" href="?page={{nextPage}}">Next Page</a>
        </li>
        {{/if}}
        {{!-- <h1>{{url}}</h1> --}}
      </ul>
    </nav>